{
  "name": "wordpress/rollbfaster-updates",
  "description": "Speeds up plugin/theme updates by moving directories rather than recursively copying files. Only for updating from 'update-core.php'.",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Andy Fragen",
      "email": "andy@thefragens.com",
      "homepage": "https://thefragens.com",
      "role": "Developer"
    },
    {
      "name": "Colin Stewart",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/WordPress/rollbfaster-updates/issues",
    "source": "https://github.com/WordPress/rollbfaster-updates"
  },
  "prefer-stable": true,
  "require": {
    "php": ">=5.6"
  },
  "autoload": {
    "psr-4": {
      "Faster_Updates\\": "src"
    },
    "files": [
      "src/Functions/move-dir.php"
    ]
  }
}
